<%- include(`header`) -%>     
   <div class="box heading" >

       <h1> <%= title %> </h1>
       
   </div>

   <!-- <div class="categories">
        <div class="box heading">
            <h1>Categories</h1>
            
       </div>
       
   </div> -->
   
   <div class="box">

        <% items.forEach(i => { %>
            
            <div class="item">
                <input type="checkbox">
                <p> <%= i %> </p>
            </div>
     
        <% }) %>    

        <form class="item" action="/post" method="post">
            <input type="text" name="newItem" placeholder="Add New Task" autocomplete="off">
            <button type="submit" name="button" value="<%= title %>">+</button>
        </form>
        
   </div>

<%- include(`footer`) -%> 
     

 